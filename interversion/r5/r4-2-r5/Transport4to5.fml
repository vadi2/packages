/// url = 'http://hl7.org/fhir/StructureMap/Transport4to5'
/// name = 'Transport4to5'
/// title = 'FML Conversion for Transport: R4 to R5'

conceptmap "TransportPriority" {
  prefix s = "http://hl7.org/fhir/4.0/request-priority"
  prefix t = "http://hl7.org/fhir/request-priority"

  s:"routine" - t:"routine"
  s:"urgent" - t:"urgent"
  s:"asap" - t:"asap"
  s:"stat" - t:"stat"
}

conceptmap "TransportStatus" {
  prefix s = "http://hl7.org/fhir/4.0/transport-status"
  prefix t = "http://hl7.org/fhir/transport-status"

  s:"in-progress" - t:"in-progress"
  s:"completed" - t:"completed"
  s:"abandoned" - t:"abandoned"
  s:"cancelled" - t:"cancelled"
  s:"planned" - t:"planned"
  s:"entered-in-error" - t:"entered-in-error"
}

uses "http://hl7.org/fhir/4.0/StructureDefinition/Transport" alias TransportR4 as source
uses "http://hl7.org/fhir/StructureDefinition/Transport" alias TransportR5 as target

imports "http://hl7.org/fhir/StructureMap/*4to5"

group Transport(source src : TransportR4, target tgt : TransportR5) extends DomainResource <<type+>> {
  src.identifier -> tgt.identifier;
  src.instantiatesCanonical -> tgt.instantiatesCanonical;
  src.instantiatesUri -> tgt.instantiatesUri;
  src.basedOn -> tgt.basedOn;
  src.groupIdentifier -> tgt.groupIdentifier;
  src.partOf -> tgt.partOf;
  src.status as v -> tgt.status = translate(v, '#TransportStatus', 'code');
  src.statusReason -> tgt.statusReason;
  src.intent -> tgt.intent;
  src.priority as v -> tgt.priority = translate(v, '#TransportPriority', 'code');
  src.code -> tgt.code;
  src.description -> tgt.description;
  src.focus -> tgt.focus;
  src.for -> tgt.for;
  src.encounter -> tgt.encounter;
  src.completionTime -> tgt.completionTime;
  src.authoredOn -> tgt.authoredOn;
  src.lastModified -> tgt.lastModified;
  src.requester -> tgt.requester;
  src.performerType -> tgt.performerType;
  src.owner -> tgt.owner;
  src.location -> tgt.location;
  src.insurance -> tgt.insurance;
  src.note -> tgt.note;
  src.relevantHistory -> tgt.relevantHistory;
  src.restriction as s -> tgt.restriction as t then TransportRestriction(s,t);
  src.input as s -> tgt.input as t then TransportInput(s,t);
  src.output as s -> tgt.output as t then TransportOutput(s,t);
  src.requestedLocation -> tgt.requestedLocation;
  src.currentLocation -> tgt.currentLocation;
  src.reason -> tgt.reason;
  src.history -> tgt.history;
}

group TransportRestriction(source src, target tgt) extends BackboneElement {
  src.repetitions -> tgt.repetitions;
  src.period -> tgt.period;
  src.recipient -> tgt.recipient;
}

